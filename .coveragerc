[run]
branch = true
source =
    quartet_capture

[report]
omit =
    *site-packages*
    *tests*
    *.tox*
    *errors.py
    *permissions.py
    *management*
    ./quartet_capture/apps.py
    ./quartet_capture/admin.py
show_missing = True
exclude_lines =
    raise NotImplementedError
    def create_and_queue_task*
